{
  "name": "react-vite-template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "vite --config config/vite.config.ts --mode=dev",
    "start:uat": "vite --config config/vite.config.ts --mode=uat",
    "start:prod": "vite --config config/vite.config.ts --mode=prod",
    "debug": "vite --config config/vite.config.ts --mode=dev debug",
    "build": "vite --config config/vite.config.ts build",
    "preview": "vite --config config/vite.config.ts --mode=dev preview",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "postinstall": "patch-package",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "start:analyzer": "cross-env ANALYZE=1 umi dev",
    "build:analyzer": "cross-env ANALYZE=1 umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-table": "^3.0.16",
    "@formily/antd": "^2.1.10",
    "@formily/core": "^2.1.10",
    "@formily/react": "^2.1.10",
    "@monaco-editor/react": "^4.4.5",
    "@reduxjs/toolkit": "^1.8.4",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/pinyin": "^2.10.0",
    "@types/react-transition-group": "^4.4.4",
    "ahooks": "^3.7.0",
    "ahooks-v2": "^2.10.15",
    "antd": "^4.23.6",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "echarts": "^5.3.2",
    "echarts-for-react": "^3.0.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "monaco-editor": "^0.33.0",
    "pinyin": "^2.10.2",
    "qrcode": "^1.5.0",
    "qs": "^6.11.0",
    "rc-resize-observer": "^1.2.0",
    "react": "18.x",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "18.x",
    "react-draggable": "^4.4.5",
    "react-infinite-scroll-component": "^6.1.0",
    "react-mentions": "^4.4.4",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.2",
    "react-resizable": "^3.0.4",
    "react-router": "~6.3.0",
    "react-router-dom": "~6.3.0",
    "react-shadow": "^19.0.3",
    "react-transition-group": "^4.4.2",
    "uuid": "^8.3.2",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/qrcode": "^1.4.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.6",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.0",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-mentions": "^4.1.6",
    "@types/react-resizable": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "@umijs/fabric": "^3.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.1.0",
    "less": "^4.1.3",
    "lint-staged": "^10.0.7",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.0",
    "stylelint": "^13.0.0",
    "tailwindcss": "^3.x",
    "typescript": "^4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-inspect": "^0.7.7",
    "vite-tsconfig-paths": "^3.5.1"
  }
}
